<Page x:Class="ZPSB_SPAGETTI.View.Pages.GamePage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:ZPSB_SPAGETTI.View.Pages"
      xmlns:game="clr-namespace:ZPSB_SPAGETTI.View.UserControls.Game"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="GamePage">

    <DockPanel LastChildFill="True">
        <game:GameHabitList DockPanel.Dock="Right"/>
        <game:GameSpace DockPanel.Dock="Left"/>
    </DockPanel>
</Page>
