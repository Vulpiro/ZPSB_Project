<UserControl x:Class="ZPSB_SPAGETTI.View.UserControls.Game.GameEnemyTile"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ZPSB_SPAGETTI.View.UserControls.Game"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid Background="Brown">
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Image />
        <TextBlock Text="Enemy" Grid.Row="1"/>
        <TextBlock Text="HP" Grid.Row="2"/>
    </Grid>
</UserControl>
